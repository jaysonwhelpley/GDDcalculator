<p id="notice"><%= notice %></p>

<h1>Sentinels</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sentinels.each do |sentinel| %>
      <tr>
        <td><%= link_to 'Show', sentinel %></td>
        <td><%= link_to 'Edit', edit_sentinel_path(sentinel) %></td>
        <td><%= link_to 'Destroy', sentinel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sentinel', new_sentinel_path %>
